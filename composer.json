{
    "name": "hop4rt/zonframework",
    "description": "Zon Framework",
    "type": "library",
    "keywords": [
        "framework",
        "zon"
    ],
    "homepage": "https://github.com/hop4rt/zonframework",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.3.29 <=7.0.33"
    },
    "autoload": {
        "psr-0": {
            "Zend_": "library/"
        }
    },
    "include-path": [
        "library/"
    ],
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "5.3.29"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "phpunit/dbunit": "1.3.*",
        "symfony/polyfill-ctype": "v1.19.0"
    },
    "archive": {
        "exclude": ["/demos", "/documentation", "/tests", "/standards", "/phpcs.xml"]
    }
}
