{
    "name": "hop4rt/zonframework",
    "description": "Zend Framework 1",
    "type": "library",
    "keywords": [
        "framework",
        "zf1"
    ],
    "homepage": "https://github.com/hop4rt/zonframework",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.3.29 <7.1.0"
    },
    "autoload": {
        "psr-0": {
            "Zend_": "library/"
        }
    },
    "include-path": [
        "library/"
    ],
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "zf-1.12.21dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "phpunit/dbunit": "1.3.*",
        "symfony/polyfill-ctype": "v1.19.0"
    },
    "archive": {
        "exclude": ["/demos", "/documentation", "/tests"]
    }
}
